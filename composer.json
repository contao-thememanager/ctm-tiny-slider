{
  "name": "contao-thememanager/ctm-tiny-slider",
  "type": "contao-bundle",
  "description": "Tiny Slider integration for Contao Theme-Manager",
  "keywords": ["slider", "theme"],
  "homepage": "https://www.oveleon.de/",
  "license": "MIT",
  "authors": [
    {
      "name": "Oveleon",
      "homepage": "https://oveleon.de/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || 5.1.*",
    "contao-thememanager/core": "^2.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemeManager\\TinySlider\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.0.x-dev"
    },
    "contao-manager-plugin": "ContaoThemeManager\\TinySlider\\ContaoManager\\Plugin"
  }
}
